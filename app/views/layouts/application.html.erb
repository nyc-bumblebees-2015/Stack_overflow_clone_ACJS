<!DOCTYPE html>
<html>
<head>
  <title>StackOverflowClone</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if flash[:error]%>
  <% flash[:error].each do |error| %>
    <%= error %><br>
  <% end %>
<% end %>
<%= render 'layouts/header' %>
<div id='body-content'>
<%= yield %>
</div>
</body>
</html>
