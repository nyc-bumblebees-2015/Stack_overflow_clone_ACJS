<%= link_to "Home", root_path %>
<% if current_user %>
  <%= @current_user.username%>
  <%= link_to "Profile", user_path(session[:user_id]) %>
  <%= link_to "Logout", logout_path %>
<% else %>
  <%= link_to "Sign up", signup_path %>
  <%= link_to "Login", login_path %>
<% end %>
<%= flash[:notice]%>
